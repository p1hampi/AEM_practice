<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
          jcr:primaryType="rep:ACL">
    <deny
            jcr:primaryType="rep:DenyACE"
            rep:principalName="projects-administrators"
            rep:privileges="{Name}[jcr:all]"/>
    <deny1
            jcr:primaryType="rep:DenyACE"
            rep:principalName="projects-users"
            rep:privileges="{Name}[jcr:all]"/>
    <allow2
            jcr:primaryType="rep:GrantACE"
            rep:principalName="projects-aem_wknd_transalation_projct"
            rep:privileges="{Name}[jcr:read,jcr:readAccessControl]"/>
    <allow3
            jcr:primaryType="rep:GrantACE"
            rep:principalName="projects-aem_wknd_transalation_projct-owner"
            rep:privileges="{Name}[jcr:all]"/>
    <allow4
            jcr:primaryType="rep:GrantACE"
            rep:principalName="projects-aem_wknd_transalation_projct-editor"
            rep:privileges="{Name}[rep:write]">
        <rep:restrictions
                jcr:primaryType="rep:Restrictions"
                rep:glob="/*"/>
    </allow4>
    <allow5
            jcr:primaryType="rep:GrantACE"
            rep:principalName="projects-aem_wknd_transalation_projct-translationreviewer"
            rep:privileges="{Name}[jcr:read,jcr:readAccessControl]"/>
    <allow6
            jcr:primaryType="rep:GrantACE"
            rep:principalName="projects-aem_wknd_transalation_projct-translationproducer"
            rep:privileges="{Name}[jcr:all]"/>
    <allow7
            jcr:primaryType="rep:GrantACE"
            rep:principalName="projects-aem_wknd_transalation_projct-translationprojectmanager"
            rep:privileges="{Name}[jcr:all]"/>
</jcr:root>
